{
  "name": "deepcut",
  "version": "1.0.0",
  "description": "DeepCut - An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "build:whisper": "cd external/whisper.cpp/examples/addon.node && npm i && cd ../../ && npx cmake-js compile -T addon.node -B Release",
    "postinstall": "npm run build:whisper && electron-builder install-app-deps"
  },
  "dependencies": {
    "@deepgram/sdk": "^4.5.0",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@google/genai": "^1.5.1",
    "@libsql/client": "^0.15.9",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@trpc/client": "10.45.1",
    "@trpc/react-query": "10.45.1",
    "@trpc/server": "10.45.1",
    "better-sqlite3": "^11.0.0",
    "drizzle-orm": "^0.44.3",
    "electron-trpc": "^0.7.1",
    "electron-updater": "^6.3.9",
    "fast-xml-parser": "^4.3.6",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "twitter-api-v2": "^1.24.0",
    "yt-dlp-wrap": "^2.3.12",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "4.18.0",
    "@types/node": "24.0.14",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmake-js": "^7.3.1",
    "drizzle-kit": "^0.31.4",
    "electron": "37.2.1",
    "electron-builder": "26.0.12",
    "electron-vite": "^4.0.0",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "node-gyp": "^11.2.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-player": "^2.16.0",
    "react-router-dom": "^7.6.3",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "vite": "7.0.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "@tailwindcss/oxide",
      "better-sqlite3"
    ]
  }
}